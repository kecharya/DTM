@model DischargeTransportManagement.Models.Destination

<div class="container">


        <div class="content-wrapper">
            <div class="panel panel-default" id="contactPanel">
                <div class="panel-body">

                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <div class="pv-lg">
                                <h3 class="m0 text-bold">New Destination</h3>
                            </div>
                        </div>
                        <div class="col-sm-12" id="contactDetails">
                            <form class="form-horizontal p-20">
                                <div class="form-group">
                                    <div class="col-sm-4">Name</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtName" name="DestinationName" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">Address</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtAddress" name="Address" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">City</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtCity" name="City" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">County</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtCounty" name="County" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">State</div>
                                    <div class="col-sm-8">
                                        @Html.DropDownList("State", Model.StatesList, new { @class = "form-control" })
                                        @*<input type="text" id="txtState" name="State" class="form-control" required />*@
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">Zip</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtZip" name="Zip" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-4">Phone</div>
                                    <div class="col-sm-8">
                                        <input type="text" id="txtPhone" name="Phone" class="form-control" required />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="pull-right" style="margin-right: 10px;">
                                        <label style="color:red;" id="lblError"></label>
                                        <input type="submit" value="Add Destination" class="btn btn-success" />
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>


</div>



